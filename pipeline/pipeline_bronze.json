					"scripts": [
						{
							"type": "NonQuery",
							"text": "-- 1. CATEGORY\nDROP TABLE IF EXISTS [bronze].[category];\nCREATE TABLE [bronze].[category] (\n    [category_id] varchar(100),\n    [category_name] varchar(100),\n    [extra_note] varchar(100)\n);\n\n-- 2. PRODUCTS\nDROP TABLE IF EXISTS [bronze].[products];\nCREATE TABLE [bronze].[products] (\n    [product_id] varchar(100),\n    [product_name] varchar(200),\n    [category_id] varchar(100),\n    [launch_date] varchar(100),\n    [price] varchar(100),\n    [notes] varchar(100)\n);\n\n-- 3. STORES\nDROP TABLE IF EXISTS [bronze].[stores];\nCREATE TABLE [bronze].[stores] (\n    [store_id] varchar(100),\n    [store_name] varchar(200),\n    [city] varchar(100),\n    [country] varchar(100),\n    [opened] varchar(100)\n);\n\n-- 4. SALES\nDROP TABLE IF EXISTS [bronze].[sales];\nCREATE TABLE [bronze].[sales] (\n    [sale_id] varchar(100),\n    [sale_date] varchar(100),\n    [store_id] varchar(100),\n    [product_id] varchar(100),\n    [quantity] varchar(100)\n);\n\n-- 5. WARRANTY\nDROP TABLE IF EXISTS [bronze].[warranty];\nCREATE TABLE [bronze].[warranty] (\n    [claim_id] varchar(100),\n    [claim_date] varchar(100),\n    [sale_id] varchar(100),\n    [repair_status] varchar(100)\n);"
						}
					],
					"scriptBlockExecutionTimeout": "02:00:00"
				}
